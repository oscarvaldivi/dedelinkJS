<%- include ("componentes/header", {titulo: "Encuentra Freelancers"}) %>
    <main>
      <section id="superior">
        <div id="imagen-superior" class="jumbotron jumbotron-fluid">
          <!-- <img src="images/dedelink_imagen_portada.jpg"> -->
          <div class="container text-center py-5 text-light">
            <h2 class="mb-4">
              Los mejores freelances <br />
              para trabajar en tu proyecto
            </h2>
            <form>
              <div class="row no-gutters bg-white p-2 rounded">
                <div class="col mr-2">
                  <input id="mySearch"
                    placeholder="Encontrar un freelance..."
                    type="text"
                    class="form-control" onkeyup="myFunction">
                </div>
                <div class="col-2">
                  <button class="btn btn-secondary w-100" type="submit">
                    Buscar
                  </button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </section>
      <section id="medio" class="container">
        
        <div class="mb-5" id="botonCta">
          <button class="btn" id="botom1">Soy Freelance</button>
          <button class="btn" id="botom2">Soy tu mam√°</button>
        </div>
      </section>
      <section id="zonaVip" class="container">
        <div class="row">
          <% freelancers.filter(e=>e.especialidad==="Backend").forEach(function(freelancer){ %>
            
            <%- include ("componentes/card_freelancer",{freelancer}) %>

          <%}) %>
        </div>
      </section>

      <section>
        <!-- Un aside para el Blog (opcional) -->
      </section>
    </main>
<%- include ("componentes/footer") %>